{
    "id": "lulzbot_base",
    "version": 2,
    "name": "LulzBot",
    "inherits": "fdmprinter",
    "metadata": {
        "author": "Aleph Objects, Inc.",
        "category": "LulzBot",
        "manufacturer": "Aleph Objects, Inc.",
        "visible": false,
        "has_materials": true,
        "first_start_actions": ["MachineSettingsAction"],
        "file_formats": "text/x-gcode;application/x-stl-ascii;application/x-stl-binary;application/x-wavefront-obj;application/x3g"
    },
    
    "settings": {
        "material": {
            "children": {
                "material_print_temperature":
                {
                    "label": "Printing Temperature",
                    "description": "The temperature used for printing. Set at 0 to pre-heat the printer manually.",
                    "unit": "°C",
                    "type": "float",
                    "default_value": 210,
                    "minimum_value": "0",
                    "maximum_value": "300",
                    "maximum_value_warning": "295",
                    "enabled": true,
                    "settable_per_mesh": false,
                    "settable_per_extruder": true
                },
                "material_print_temperature_cat": {
                    "children":
                    {
                        "material_soften_temperature":
                        {
                            "label": "Soften Temperature",
                            "description": "The temperature used before z-axis homing.",
                            "unit": "°C",
                            "type": "float",
                            "default_value": 170,
                            "enabled": true,
                            "settable_per_mesh": false,
                            "settable_per_extruder": true
                        },
                        "material_probe_temperature":
                        {
                            "label": "Probe Temperature",
                            "description": "The temperature used before probing.",
                            "unit": "°C",
                            "type": "float",
                            "default_value": 160,
                            "enabled": true,
                            "settable_per_mesh": false,
                            "settable_per_extruder": true
                        },
                        "material_wipe_temperature":
                        {
                            "label": "Wipe Temperature",
                            "description": "The temperature used before nozzle wiping.",
                            "unit": "°C",
                            "type": "float",
                            "default_value": 160,
                            "minimum_value": "0",
                            "maximum_value_warning": "300",
                            "enabled": true,
                            "settable_per_mesh": false,
                            "settable_per_extruder": true
                        }
                    }
                },
                "material_bed_temperature_cat": {
                    "children":
                    {
                        "material_part_removal_temperature":
                        {
                            "label": "Part Removal Temperature",
                            "description": "The ideal bed temperature to remove the model after printing.",
                            "unit": "°C",
                            "type": "float",
                            "default_value": 50,
                            "minimum_value": "0",
                            "maximum_value": "110",
                            "enabled": true,
                            "settable_per_mesh": false,
                            "settable_per_extruder": false
                        }
                    }
                }
            }
        } 
    },
    
    "overrides": {
        "material_diameter": {
            "default_value": 2.75
        },
        "material_soften_temperature": {
            "enabled": "True"
        },
        "material_probe_temperature": {
            "enabled": "True"
        },
        "material_wipe_temperature": {
            "enabled": "True"
        },
        "material_part_removal_temperature": {
            "enabled": "True"
        }
    }
    
    
    
}
